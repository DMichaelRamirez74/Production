@model FingerprintsModel.Screening
<script type="text/javascript">
    $(document).ready(function () {
        $('input').keypress(function () {
            if ($(this).val() == 'MM/DD/YYYY') {
                $(this).val('');
                // $(this).mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
            }
        });
        $('#s067Date').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        $('#sRescreenTCRDate').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        $('#s072RescreenTCRDate').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        $('#s075ReferDate').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        $('#s076DCDate').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        $('#s079DCDate2').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        if ('@Model.s069Details' == "")
            $('#s069Details').val("");
        if ('@Model.s070Results' == "")
            $('#s070Results').val("");
        if ('@Model.s073RescreenTCRResults' == "")
            $('#s073RescreenTCRResults').val("");
        if ('@Model.s074ReferralDC' == "")
            $('#s074ReferralDC').val("");
        if ('@Model.s078DetailDC' == "")
            $('#s078DetailDC').val("");
        if ('@Model.s080DetailDC2' == "")
            $('#s080DetailDC2').val("");
        if ('@Model.s069Details' == "")
            $('#s069Details').val("");
        if ('@Model.s081FOStatus' == "")
            $('#s081FOStatus').val("");
        if ('@Model.d056evReferral' == "")
            $('#d056evReferral').val("");
        if ('@Model.s071RescreenTCR' == "")
            $('#s071RescreenTCR').val("");
    });
</script>


<div class="row add-useragform1" >
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 add-useragform">

        <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Speech Screening Date <sup>*</sup> </label>
                    <div class="date custom-time">
                        <input class="form-control" id="s067Date" maxlength="50" name="s067Date" onblur="return validatedate1(this);" placeholder="MM/DD/YYYY" type="text" value="@Model.s067Date" @if (!string.IsNullOrEmpty(Model.s067Date)){ <text> readonly="readonly" </text>    } />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Speech Screening Results </label>

                    <select name="s070Results" class="form-control" id="s070Results">

                        <option value="1" @if (@Model.s070Results == "1") { <text> selected="selected" </text>               }>Pass</option>
                        <option value="2" @if (@Model.s070Results == "2") { <text> selected="selected" </text>               }>Fail</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Speech Screener Name </label>
                    <div class="date custom-time">
                        <input class="form-control" id="s068NameTCR" maxlength="100" name="s068NameTCR" type="text" value="@Model.s068NameTCR" />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Speech Screening Details </label>

                    <select name="s069Details" class="form-control" id="s069Details">

                        <option value="1" @if (@Model.s069Details == "1") { <text> selected="selected" </text>              }>Referred</option>
                        <option value="2" @if (@Model.s069Details == "2") { <text> selected="selected" </text>              }>Schedule for Evaluation</option>
                        <option value="3" @if (@Model.s069Details == "3") { <text> selected="selected" </text>              }>IEP</option>
                        <option value="4" @if (@Model.s069Details == "4") { <text> selected="selected" </text>              }>Private Speech Therapy</option>
                    </select>
                </div>
            </div>
           
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Does Child Need Speech Rescreen with Teacher </label>

                    <select name="s071RescreenTCR" class="form-control" id="s071RescreenTCR">

                        <option value="1" @if (@Model.s071RescreenTCR == "1") { <text> selected="selected" </text>              }>Yes, After 2nd Developmental Rescreen</option>
                        <option value="2" @if (@Model.s071RescreenTCR == "2") { <text> selected="selected" </text>              }>Yes, After 2nd Developmental Rescreen w/Interpreter</option>
                        <option value="3" @if (@Model.s071RescreenTCR == "3") { <text> selected="selected" </text>              }>Yes, After 2nd Guardian Response to Teacher Screening</option>
                        <option value="4" @if (@Model.s071RescreenTCR == "4") { <text> selected="selected" </text>              }>Yes, Within Two Weeks</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Teacher Rescreen Date </label>
                    <div class="date custom-time">
                        <input class="form-control" id="s072RescreenTCRDate" maxlength="50" name="s072RescreenTCRDate" onblur="return validatedate1(this);" placeholder="MM/DD/YYYY" type="text" value="@Model.s072RescreenTCRDate" />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Teacher Rescreen Results </label>

                    <select name="s073RescreenTCRResults" class="form-control" id="s073RescreenTCRResults">

                        <option value="1" @if (@Model.s073RescreenTCRResults == "1") { <text> selected="selected" </text>              }>Pass</option>
                        <option value="2" @if (@Model.s073RescreenTCRResults == "2") { <text> selected="selected" </text>              }>Fail</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Does Child Need a Referral for Speech Rescreen with Disabilities Coordinator </label>

                    <select name="s074ReferralDC" class="form-control" id="s074ReferralDC">

                        <option value="1" @if (@Model.s074ReferralDC == "1") { <text> selected="selected" </text>              }>Yes</option>
                        <option value="2" @if (@Model.s074ReferralDC == "2") { <text> selected="selected" </text>              }>No</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Speech Referral Date </label>
                    <div class="date custom-time">
                        <input class="form-control" id="s075ReferDate" maxlength="50" name="s075ReferDate" onblur="return validatedate1(this);" placeholder="MM/DD/YYYY" type="text" value="@Model.s075ReferDate" />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Disabilities Coordinator Screening Date </label>
                    <div class="date custom-time">
                        <input class="form-control" id="s076DCDate" maxlength="50" name="s076DCDate" onblur="return validatedate1(this);" placeholder="MM/DD/YYYY" type="text" value="@Model.s076DCDate" />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Disabilities Coordinator Name </label>
                    <div class="date custom-time">
                        <input class="form-control" id="s077NameDC" maxlength="100" name="s077NameDC" type="text" value="@Model.s077NameDC" />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Disabilities Coordinator Screening Result </label>

                    <select name="s078DetailDC" class="form-control" id="s078DetailDC">

                        <option value="1" @if (@Model.s078DetailDC == "1") { <text> selected="selected" </text>              }>Pass</option>
                        <option value="2" @if (@Model.s078DetailDC == "2") { <text> selected="selected" </text>              }>Fail, Refer to MH for Further Testing</option>
                        <option value="3" @if (@Model.s078DetailDC == "3") { <text> selected="selected" </text>              }>Fail, Refer to Outside Agency for Speech Evaluation</option>
                        <option value="4" @if (@Model.s078DetailDC == "4") { <text> selected="selected" </text>              }>Rescreen</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Disabilities Coordinator Rescreen Date </label>
                    <div class="date custom-time">
                        <input class="form-control" id="s079DCDate2" maxlength="50" name="s079DCDate2" onblur="return validatedate1(this);" placeholder="MM/DD/YYYY" type="text" value="@Model.s079DCDate2" />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Disabilities Coordinator Rescreen Result </label>
                    <select name="s080DetailDC2" class="form-control" id="s080DetailDC2">

                        <option value="1" @if (@Model.s080DetailDC2 == "1") { <text> selected="selected" </text>              }>Pass</option>
                        <option value="2" @if (@Model.s080DetailDC2 == "2") { <text> selected="selected" </text>              }>Fail, Refer to MH for Further Testing</option>
                        <option value="3" @if (@Model.s080DetailDC2 == "3") { <text> selected="selected" </text>              }>Fail, Refer to Outside Agency for Speech Evaluation</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Speech Follow-Up Status</label>
                    <select name="s081FOStatus" class="form-control" id="s081FOStatus">

                        <option value="1" @if (@Model.s081FOStatus == "1") { <text> selected="selected" </text>              }>No Follow-Up Required</option>
                        <option value="2" @if (@Model.s081FOStatus == "2") { <text> selected="selected" </text>              }>Follow-Up Required, but Not Complete</option>
                        <option value="3" @if (@Model.s081FOStatus == "3") { <text> selected="selected" </text>              }>Follow-Up Complete</option>
                    </select>
                </div>
            </div>

        </div>

        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">


            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                <div class="form-group">
                    <label class="custom-file-upload">
                        <input name="ScreeningDocument" id="ScreeningDocument" class="form-control" onchange="ValidateSingleInputpdf(this)" style="display:none" type="file" value="">
                        <i class="fa fa-cloud-upload"></i> Add new document
                    </label>
                    <label style="margin-top: 8px;margin-left: 6px;" id="documentlabel"></label>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">


                @if (!string.IsNullOrEmpty(Model.PhysicalFileName))
                {
                    if (Model.PhysicalFileExtension == ".pdf")
                    {
                        string url = Path.GetFileNameWithoutExtension(Model.PhysicalFileName) + Guid.NewGuid().ToString() + ".pdf";
                        System.IO.FileStream file = System.IO.File.Create(Server.MapPath("~//TempAttachment//") + url);
                        url = "..\\TempAttachment\\" + url;
                        try
                        {
                            file.Write(Model.PhysicalImageByte, 0, Model.PhysicalImageByte.Length);
                            file.Close();
                        }
                        catch (Exception Ex)
                        {
                            FingerprintsModel.clsError.WriteException(Ex);
                        }
                        finally
                        {
                            file.Close();
                        }
                        <div style="display:none;" id="ScreeninguploadDocument">
                        </div>
                        <iframe class="iframestyle" src="@url" frameborder="0" scrolling="no"></iframe>
                    }
                    else
                    {
                        string imagedata = Convert.ToBase64String(Model.PhysicalImageByte);
                        <div id="ScreeninguploadDocument">
                            <script type="text/javascript">

                                $(document).ready(function () {
                                    $('#ScreeninguploadDocument').ZoomCrop(
                                  {
                                      image: "data:image/png;base64," + '@imagedata',
                                      updated: function (size, crop, position) {
                                          console.log(size, crop, position);
                                      }
                                  });
                                });
                            </script>

                        </div>

                    }
                }
                else
                {
                    <div id="ScreeninguploadDocument">
                        <script type="text/javascript">

                            $(document).ready(function () {
                                $('#ScreeninguploadDocument').ZoomCrop(
                              {
                                  image: HostedDir + "/Content/img/NoImageAvailable.jpg",
                                  updated: function (size, crop, position) {
                                      console.log(size, crop, position);
                                  }
                              });
                            });
                        </script>
                    </div>

                }





            </div>



        </div>



    </div>
</div>