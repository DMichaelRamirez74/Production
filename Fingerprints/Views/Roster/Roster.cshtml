        function GetMinutes(timeStr) {

            var str = timeStr.split(':');

            var hours = parseInt(str[0]);
            var mins = parseInt(str[1]);
            //if (hours >= 12)
            //{
            //    hours = hours - 12;
            //}
            var totalMins = hours * 60 + mins;

            return totalMins;
        }
