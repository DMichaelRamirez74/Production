@{
    ViewBag.Title = "WorkshopAnalysis";
    Layout = "~/Views/Shared/AgencyStaffLayout.cshtml";
}
@section MainContentHolder{
    <link href="~/Content/ERSEA/erseamain.css" rel="stylesheet" />
    <link href="~/Content/css/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <style>
        #page-wrapper {
            padding-left: 0;
            padding-right: 0;
            overflow: auto;
        }

        .ersa-select option {
            background: #164273 !important;
        }
    </style>
    <div class="right-side-container-ch">

        <div class="col-xs-12 no-padding">
            <div class="col-xs-12 no-padding">
                <div class="col-xs-12 ersea-detl-title">
                    <h2>Workshop Analysis</h2>
                    <div class="ersea_ovr"><img src="~/images/text-bdr.png" class="img-responsive"></div>
                </div>
                <div class="col-xs-12 text-center serch-billing-block" id="multipleCenter" style="display:none;">
                    <select name="" class="ersa-select select_center"></select>
                    <div class="ersa-search-btn">
                        <a href="javascript:void(0);" id="searchByCenter">Search</a>
                    </div>
                </div>

                <div class="col-xs-12 text-center serch-billing-block" id="singleCenterBlock" style="display:none;">
                    <label style="display: initial;color: #f9c751;font-size: 18px;">Center Name: </label>
                    <span id="centernameSpan" style="color: #f1f1f1;font-size: 18px;"></span>
                </div>
                <div class="col-xs-12">
                    <div class="ersa_title">
                        <h4>Search Results</h4>
                    </div>
                </div>
                <div class="col-xs-12">
                    <div class="col-xs-12 append_WorkshopDiv mCustomScrollbar light" data-mcs-theme="minimal-dark" style="border-radius: 10px;background: #f1f5f8;padding: 10px;width: 100%;margin:20px 0;border-top-left-radius: 0;border-top-right-radius: 0;margin-top: 0;height:300px;overflow:scroll;">
                    </div>

                </div>
            </div>

        </div>
    </div>

    <!---Div Workshop Div-->
    <div class="append_Workop hidden">
        <div class="col-lg-4 col-xs-12 no-padding">
            <div class="ersa-content-desc" style="position: relative;">
                <p class="workshop-name"></p>
                <span class="house-enrolled"></span>
            </div>
        </div>
    </div>

    <script src="~/Scripts/workshopanalysis.js"></script>

}