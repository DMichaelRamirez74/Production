    var _validFileExtensions = [".pdf", ".jpg", ".jpeg", ".bmp", ".gif", ".png"];
    function ValidateFileinput4(oInput) {
        //  $(oInput).parent().next('Div').find('input[type=checkbox]').prop("checked", false);
        if (oInput.type == "file") {
            var sFileName = oInput.value;
            if (sFileName.length > 0) {
                var blnValid = false;
                //var v = oInput.files[0].size / 1024 / 1024;
                var valid_size = 3 * 1000 * 1000;
                var v = oInput.files[0].size;
                //if (v <= 1) {
                if (v <= valid_size) {
                    for (var j = 0; j < _validFileExtensions.length; j++) {
                        var sCurExtension = _validFileExtensions[j];
                        if (sFileName.substr(sFileName.length - sCurExtension.length, sCurExtension.length).toLowerCase() == sCurExtension.toLowerCase()) {
                            blnValid = true;
                            break;
                        }
                    }
                    if (!blnValid) {
                        oInput.value = "";
                        return false;
                    }
                }
                else {
                    oInput.value = "";
                    customAlertforlongtime("File size should not be greater than 1 MB");
                    return false;

                }

            }
        }
        return true;
    }
