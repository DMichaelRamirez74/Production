                            if (data == '1') {

                                customAlert('Record updated successfully');

                                $('#searchByfilter').trigger('click');
                            }

                            else if (data == '0') {
                                customAlert('Error occurred.Please, try again later');
                            }

                        }

                    },
                    error: function (data) {

                    },
                    complete: function (data) {
                        $('#spinner').hide();
                    }


                });


            }



        }


    </script>


}

