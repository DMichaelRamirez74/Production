@model FingerprintsModel.ERSEADashBoard
@{
   
    Layout = "~/Views/Shared/AgencyStaffLayout.cshtml";
    int i = 0;
    int j = 0;
}
@section Title{ 

ERSEA Dashboard
}
@section MainContentHolder{
    <link href="~/Content/ERSEA/erseamain.css" rel="stylesheet" />
    <link href="~/Content/css/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <style>
        #page-wrapper {
            padding-left: 0;
            padding-right: 0;
        }

        .scroll-thead th {
            border: 1px solid #f1f5f8;
        }

        .scroll-thead {
            display: table;
            width: 100%;
            table-layout: fixed;
            width: calc( 100% - 17px );
        }

        .scroll-tbody tr td, .scroll-tbody1 tr td {
            word-break: break-word;
        }

        .scroll-tbody {
            display: block;
            height: 250px;
            overflow: auto;
            overflow-x: hidden;
        }

            .scroll-tbody tr {
                display: table;
                width: 100%;
                table-layout: fixed;
            }

                .scroll-tbody tr td, .scroll-tbody1 tr td {
                    word-break: break-word;
                }

        .scroll-tbody1 {
            display: block;
        }

            .scroll-tbody1 tr {
                display: table;
                /*width: 100%;*/
                width: 98.5%;
                table-layout: fixed;
            }

        .ersea-table thead tr th {
            font-size: 16px;
        }

        .top-header-title-ersea {
            margin-top: 30px;
        }

        .pagination-ch {
            padding: 6px 11px;
            height: 33px;
            background: #fff;
            display: inline-block !important;
            cursor: pointer;
            vertical-align: top;
        }

            .pagination-ch i {
                font-size: 17px;
                padding-top: 3px;
            }


        .scroll-thead
        {
            width: 100%!important;
        }

        .ersea-table thead tr td
        {
            font-size: 14px!important;
        }

        .ersea-table thead tr th
        {
            font-size: 14px!important;
        }

        .scroll-tbody1 tr
        {
            width: 100%!important;
        }

        .ersea-table .scroll-tbody{
            max-height: 300px;
            height:auto;
        }

        @@media only screen and (max-width: 768px) {
            .ersea-table .scroll-tbody {
                width: 100%;
                max-height:none !important;
            }
