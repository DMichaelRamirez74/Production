
@{
    ViewBag.Title = "EducationManagerDashboard";
    Layout = "~/Views/Shared/AgencyStaffLayout.cshtml";
}





@section Script{
<script src="~/Scripts/amcharts.js"></script>
<script src="~/Content/ckeditor/ckeditor.js"></script>
<script src="~/Scripts/gauge.js"></script>
<script src="~/Scripts/serial.js"></script>
<script src="~/Scripts/export.min.js"></script>
<link href="~/Content/css/export.css" rel="stylesheet" />
<script src="~/Scripts/light.js"></script>
<script src="~/Scripts/pie.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            GetChart();
        });


        function GetChart() {
            $.ajax({
                url: HostedDir + "/EducationManager/GetEducationManagerDashboard",
                type: "POST",

                dataType: "json",
                secureuri: false,
                async: false,
                success: function (response) {
                    console.log(response);
                    if (response != null) {
                        var chart = AmCharts.makeChart("EducationLevel", {
                            "type": "pie",
                            "startDuration": 0,
                            "theme": "light",
                            "addClassNames": true,
                            "balloon": {
                                "adjustBorderColor": true,
                                "color": "#000000",
                                // "cornerRadius": 5,
                                "fontSize": 14,
                                "maxWidth": 320,
                                "verticalPadding": 20,
                                "fillColor": "#FFFFFF"
                            },
                            "legend": {
                                "position": "right",
                                "marginRight": 10,
                                "autoMargins": false,
                                "valueTextRegular": "([[value]])"
                            },
                            "innerRadius": "20%",
                            "defs": {
                                "filter": [{
                                    "id": "shadow",
                                    "width": "300%",
                                    "height": "300%",
                                    "feOffset": {
                                        "result": "offOut",
                                        "in": "SourceAlpha",
                                        "dx": 0,
                                        "dy": 0
                                    },
                                    "feGaussianBlur": {
                                        "result": "blurOut",
                                        "in": "offOut",
                                        "stdDeviation": 5
                                    },
                                    "feBlend": {
                                        "in": "SourceGraphic",
                                        "in2": "blurOut",
                                        "mode": "normal"
                                    }
                                }]
                            },
                            "dataProvider": [


                                {
                                    "country": "High School",
                                    "litres": response.EduLevel0,
                                    "color": "#FF0F00"
                                }, {
                                    "country": "Associate",
                                    "litres": response.EduLevel2,
                                    "color": "#04D215"
                                }, {
                                    "country": "Baccalaureate",
                                    "litres": response.EduLevel3,
                                    "color": "#754DEB"
                                }, {
                                    "country": "Advanced",
                                    "litres": response.EduLevel4,
                                    "color": "#FF6600"
                                }, ],
                            "valueField": "litres",
                            "titleField": "country",
                            "colorField": "color",
                        });
                        var chart = AmCharts.makeChart("GettingDegree", {
                            "type": "pie",
                            "startDuration": 0,
                            "theme": "light",
                            "addClassNames": true,
                            "balloon": {
                                "adjustBorderColor": true,
                                "color": "#000000",
                                // "cornerRadius": 5,
                                "fontSize": 14,
                                "maxWidth": 320,
                                "verticalPadding": 20,
                                "fillColor": "#FFFFFF"
                            },
                            "legend": {
                                "position": "right",
                                "marginRight": 10,
                                "autoMargins": false,
                                "valueTextRegular": "([[value]])"
                            },
                            "innerRadius": "20%",
                            "defs": {
                                "filter": [{
                                    "id": "shadow",
                                    "width": "300%",
                                    "height": "300%",
                                    "feOffset": {
                                        "result": "offOut",
                                        "in": "SourceAlpha",
                                        "dx": 0,
                                        "dy": 0
                                    },
                                    "feGaussianBlur": {
                                        "result": "blurOut",
                                        "in": "offOut",
                                        "stdDeviation": 5
                                    },
                                    "feBlend": {
                                        "in": "SourceGraphic",
                                        "in2": "blurOut",
                                        "mode": "normal"
                                    }
                                }]
                            },
                            "dataProvider": [


                                {
                                    "country": "Getting Degree",
                                    "litres": response.GettingDegree,
                                    "color": "#FF0F00"
                                }, {
                                    "country": "Not Getting Degree",
                                    "litres": response.NotGettingDegree,
                                    "color": "#04D215"
                                } ],
                            "valueField": "litres",
                            "titleField": "country",
                            "colorField": "color",
                        });
                    }
                }
            });
        }
    </script>
}
@section MainContentHolder{
