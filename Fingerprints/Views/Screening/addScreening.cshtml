                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Custom Screening starts 12-20-2016*@


                                @{
                                    try
                                    {
                                        string i = "";
                                        string i1 = "";
                                        string filename = "";
                                        string radioname = "";
                                        string DownloadDocument = "";
                                        if (Model.CustomScreening != null && Model.CustomScreening.Rows.Count > 0)
                                        {
                                            DataTable ScreeningNames = Model.CustomScreening.DefaultView.ToTable(true, "ScreeningName", "ScreeningID", "programtype");
                                            foreach (DataRow Row in ScreeningNames.Rows)
                                            {
                                                i = @Row["ScreeningName"].ToString() + "101";
                                                i1 = @Row["ScreeningName"].ToString() + "102";
                                                filename = "k" + Row["ScreeningID"].ToString() + "k" + Row["ScreeningName"].ToString() + "_customscreeningdocument";
                                                radioname = Row["ScreeningID"].ToString() + "@" + Row["ScreeningName"].ToString() + "_allowchildcustomscreening";
                                                DownloadDocument = "Allow" + Row["ScreeningName"].ToString() + Row["ScreeningID"].ToString() + "Screening";

                                                <div class="col-lg-6 customallowscreening" accesskey="@Row["programtype"].ToString()">
                                                    <div class="form-group">
                                                        <label> Allow @Row["ScreeningName"].ToString() Screening </label>
                                                        <div class="cont-sz">
                                                            <div class="col-sm-3">
                                                                <input type="radio" class="customscreeningradioallowed" id="@i" onchange="showcustomscreening('@Row["ScreeningName"].ToString()');" name="@radioname" value="1" />
                                                                <span style="font-size: 14px;">Yes</span>
                                                            </div>
                                                            <div class="col-sm-3">
                                                                <input type="radio" class="customscreeningradioallowed" id="@i1" onchange="hidecustomscreening('@Row["ScreeningName"].ToString()');" name="@radioname" value="0" />
                                                                <span style="font-size: 14px;">No</span>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <input name="@filename" id="@filename" class=" form-control" onchange="ValidateSingleInputpdf2(this)" type="file" value="" style="width:161% !important;">
                                                            </div>
                                                            <div class="col-sm-2" style="margin-top: 2%; padding-left: 11%;">
