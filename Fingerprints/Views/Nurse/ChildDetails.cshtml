    function CalculatePercentage(el) {

        var ex = /^(100([\.][0]{1,})?$|[0-9]{1,2}([\.][0-9]{1,})?)$/;
        if (ex.test(el.value) == false) {
            el.value = "";
            $('#Amountqca1').val('');
            $('#Childincome').val($('#Amountqca2').val());
        }
        else {
