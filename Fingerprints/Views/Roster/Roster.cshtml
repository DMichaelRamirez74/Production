        function ReadMoreLessContent(minimized_elements) {
            // var minimized_elements = $('p.text-viewer');

            minimized_elements.each(function () {
                var t = $(this).html();
                if (t.length < 100) return;

                $(this).html(
                    t.slice(0, 100) + '<span>... </span><a href="javascript:void(0);" class="readmore"> Read More>> </a>' +
                    '<span style="display:none;">' + t.slice(100, t.length) + ' <a href="javascript:void(0);" class="readless"> << Less </a></span>'
                );
            });

            $('a.readmore', minimized_elements).click(function (event) {
                event.preventDefault();
                $(this).hide().prev().hide();
                $(this).next().show('slow');
            });

            $('a.readless', minimized_elements).click(function (event) {
                event.preventDefault();
                $(this).parent().hide().prev().show().prev().show();
            });

        }



