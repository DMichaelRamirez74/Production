@section Script{

    <script type="text/javascript">
        var orderid;
        $(document).ready(function () {

        
            $("#ModalTempList").on("hidden.bs.modal", function () {
       
                $('.import-agara').hide();

            });
            $("#ModalCompleteList").on("hidden.bs.modal", function () {
          
                $('.import-agara').hide();

            });
      

            $("#ModalWaitingList").on("hidden.bs.modal", function () {
       
                $('.import-agara').hide();

            });
        });
    
        function GetWorkOrderDetail(yakkrid, arg, type) {
            orderid = $(arg).attr('data-workorderid');

            $('.import-agara').show();

            $.ajax({
                url: HostedDir + "/Home/GetWorkOrderDetail",
                type: "POST",
                data: {
                    Yakkrid: yakkrid,
                    OrderId: orderid,
                },
                dataType: "json",
                secureuri: false,
                async: false,
                success: function (Data) {
                    console.log(Data);

                    BindWorkOrderDetails(Data, type);

                }
, error: function (Data) {
    customAlert("Session Ended Log Onto The System Again.");
    setTimeout(function () {
        window.location.href = HostedDir + '/login/Loginagency';
    }, 2000);
}
            });

        }


        function BindWorkOrderDetails(data, type) {
            debugger;
            var parent = "";
            if (type == 2) {
                parent = $('.OpenList');

            }
           
            else if (type == 5) {
                parent = $('.TempBind');
                //  BindTableData(data, parent);
            }
            else if (type == 6) {
                parent = $('.CompleteBind');

            }
            $('.workparts').html("");
            BindWorkList(data, parent);
            $('.spnHeading').text("Details for " + orderid)
            $(parent).find('.spnDescription').text(data.UserDescription);
            $(parent).find('.spnCenterName').text(data.CenterName);
            $(parent).find('.spnStaff').text(data.StaffName);
            $(parent).find('.spnFixedType').text(data.IsTemporaryFix);
           // $(parent).find('.spnAssignTo').text(data.AssignedTo);
            $(parent).find('.spnDate').text(data.RequestedDate);



        }

        function BindWorkList(Data, parent) {

            if (Data.workOrderList != null && Data.workOrderList.length > 0) {

                for (var j = 0; j < Data.workOrderList.length; j++) {
                    DivContent = "", tabContent = "", tabRow = "";;
                    DivContent = $('#GetAppendDiv').html();
                    DivContent = DivContent.replace('SectionTitle', "Fix Number " + (j + 1))
                    DivContent = DivContent.replace('#collapse13', "#collapse" + (j + 1))
                    DivContent = DivContent.replace('collapse13', "collapse" + (j + 1))
                    DivContent = DivContent.replace('StaffWorked', Data.workOrderList[j].StaffName);
                    DivContent = DivContent.replace('CurrentStatus', Data.workOrderList[j].IsTemporaryFix);

                    if (Data.workOrderList[j].ImageCount == 0)
                        DivContent = DivContent.replace('ImageCount', '<a href="#" >' + Data.workOrderList[j].ImageCount + '</a>');
                    else
                        DivContent = DivContent.replace('ImageCount', '<a href="#" style="text-decoration:underline;" onclick="LoadCarouselImages(' + Data.workOrderList[j].WorkId + ')">' + Data.workOrderList[j].ImageCount + '</a>');

                    DivContent = DivContent.replace('Notes123', Data.workOrderList[j].Notes);
                    DivContent = DivContent.replace('DateGiven', Data.workOrderList[j].RequestedDate);

                    var tr = "<tbody>";
                    if (Data.workOrderList[j].PartDetails != null && Data.workOrderList[j].PartDetails.length > 0) {

                        for (var i = 0; i < Data.workOrderList[j].PartDetails.length; i++) {

                            tr += "<tr>";
                            tr += '<td>' + (j + 1) + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].PartNumber + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].PartDescription + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].Quantity + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].UnitCost + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].TotalCost + '</td>';
                            tr += "</tr>";
                        }
                        DivContent = DivContent.replace("<tbody>", tr);
                    }
                    else {

                        tr += "<tr>";
                        tr += "<td colspan='6' style=' text-align: center; color: #db4437;'>No record found</td>";
                        tr += "</tr>";
                        DivContent = DivContent.replace("<tbody>", tr);
                    }
                    $(parent).find('.workparts').append(DivContent);
                }



            }
            else {

            }

        }
        function BindTableData(Data, parent) {

            $(parent).find('.tbody-tempnotes').html("");
            $(parent).find('.tbody-parts').html("");
            var tr = "";
            var tr1 = "";
            if (Data.PartDetails != null && Data.PartDetails.length > 0) {

                for (var j = 0; j < Data.PartDetails.length; j++) {

                    tr += "<tr>";
                    tr += '<td>' + (j + 1) + '</td>';
                    tr += '<td>' + Data.PartDetails[j].PartNumber + '</td>';
                    tr += '<td>' + Data.PartDetails[j].PartDescription + '</td>';
                    tr += '<td>' + Data.PartDetails[j].Quantity + '</td>';

                    tr += '<td>' + Data.PartDetails[j].UnitCost + '</td>';
                    tr += '<td>' + Data.PartDetails[j].TotalCost + '</td>';
                    tr += "</tr>";
                }
            }
            else {

                tr += "<tr>";
                tr += "<td colspan='6' style=' text-align: center; color: #db4437;'>No record found</td>";
                tr += "</tr>";

            }

            if (Data.TemporaryFixList != null && Data.TemporaryFixList.length > 0) {

                for (var j = 0; j < Data.TemporaryFixList.length; j++) {

                    tr1 += "<tr>";

                    tr1 += '<td>' + Data.TemporaryFixList[j].Notes + '</td>';
                    tr1 += '<td>' + Data.TemporaryFixList[j].CreatedDate + '</td>';
                    tr1 += '<td>' + Data.TemporaryFixList[j].NotesEnteredBy + '</td>';
                    tr1 += "</tr>";
                }
            }
            else {

                tr1 += "<tr>";
                tr1 += "<td colspan='3' style=' text-align: center; color: #db4437;'>No record found</td>";
                tr1 += "</tr>";

            }
            $(parent).find('.tbody-parts').append(tr);
            $(parent).find('.tbody-tempnotes').append(tr1);
        }
        function LoadCarouselImages(workid) {
            $.ajax({
                url: HostedDir + "/Home/LoadCarouselImages",
                type: "POST",
                data: {
                    workid: workid
                },
                dataType: "json",
                secureuri: false,
                async: false,
                success: function (Data) {
                    $('.carousel-inner').html("");
                    $('#CarouselTitle').text("Images for " + orderid);
                    for (var i = 0 ; i < Data.length; i++) {
                        var div = "";
                        if (i == 0)
                            div = '<div class="item active"> <img class="roundimage ZoomImage" src="data:image/jpg;base64,' + Data[i] + '" /> </div>';
                        else
                            div = '<div class="item"> <img class="roundimage ZoomImage" src="data:image/jpg;base64,' + Data[i] + '" /> </div>';

                        $('.carousel-inner').append(div);
                    }

                    $('#ModalCarousel').modal('show');

                }
, error: function (Data) {
    customAlert("Session Ended Log Onto The System Again.");
    setTimeout(function () {
        window.location.href = HostedDir + '/login/Loginagency';
    }, 2000);
}
            });

        }
        function LoadWorkOrderList(arg, type) {
            $.ajax({
                url: HostedDir + "/Home/GetStaffWorkOrderStatusList",
                type: "POST",
                data: {
                    Centerid: $(arg).attr('data-centerid'),
                    Type: type,
                },
                dataType: "json",
                secureuri: false,
                async: false,
                success: function (Data) {
                    switch (type) {
                        case 1:
                            BindOpennedlist(Data);
                            break;
                        case 2:
                            BindTempFix(Data);
                            break;
                        case 3:
                            BindCompletedFix(Data);
                            break;
                       
                    }

                }
, error: function (Data) {
    customAlert("Session Ended Log Onto The System Again.");
    setTimeout(function () {
        window.location.href = HostedDir + '/login/Loginagency';
    }, 2000);
}
            });
        }

        function BindCompletedFix(Data) {
            console.log(Data);
            if (Data != null && Data.length > 0) {
                $('#div5').hide();
                var tbody = "";
                var option = "";
                $('#DivModalCompleteList > tbody > tr').remove();
                for (var i = 0; i < Data.length; i++) {
                    tbody = tbody + '<tr><td style="text-decoration: underline;cursor:pointer;" data-workorderid=' + Data[i].workorderSequence + ' onclick="GetWorkOrderDetail(' + Data[i].YakkrId + ',this,6)">' + Data[i].workorderSequence + '</td>';
                    tbody = tbody + '<td>' + Data[i].YakkrCode + ' & ' + Data[i].yakkrdescription + '</td>';
                    tbody = tbody + '<td>' + Data[i].StaffName + '</td> ';
                    tbody = tbody + '<td>' + Data[i].RequestedDate + '</td> ';

                    tbody = tbody + '</tr>';
                }
                $('#DivModalCompleteList > tbody').append(tbody);
                if (!$('#ModalCompleteList').hasClass('in'))
                    $('#ModalCompleteList').modal('toggle');
            }
            else {
                $('#ModalCompleteList > tbody > tr').remove();
                $('#div5').show();
            }
        }
        function BindTempFix(Data) {
            console.log(Data);
            if (Data != null && Data.length > 0) {
                $('#div4').hide();
                var tbody = "";
                var option = "";
                $('#DivModalTempList > tbody > tr').remove();
                for (var i = 0; i < Data.length; i++) {
                    tbody = tbody + '<tr><td style="text-decoration: underline;cursor:pointer;" data-workorderid=' + Data[i].workorderSequence + ' onclick="GetWorkOrderDetail(' + Data[i].YakkrId + ',this,5)">' + Data[i].workorderSequence + '</td>';
                    tbody = tbody + '<td>' + Data[i].YakkrCode + ' & ' + Data[i].yakkrdescription + '</td>';
                    tbody = tbody + '<td>' + Data[i].StaffName + '</td> ';
                    tbody = tbody + '<td>' + Data[i].RequestedDate + '</td> ';
                    tbody = tbody + '</tr>';
                }
                $('#DivModalTempList > tbody').append(tbody);
                if (!$('#ModalTempList').hasClass('in'))
                    $('#ModalTempList').modal('toggle');
            }
            else {
                $('#ModalTempList > tbody > tr').remove();
                $('#div4').show();
            }
        }

        function BindOpennedlist(Data) {
            console.log(Data);
            if (Data != null && Data.length > 0) {
                $('#div5').hide();
                var tbody = "";
                var option = "";
                $('#DivWaitingListPopup > tbody > tr').remove();
                for (var i = 0; i < Data.length; i++) {
                    tbody = tbody + '<tr><td style="text-decoration: underline;cursor:pointer;" data-workorderid=' + Data[i].workorderSequence + ' onclick="GetWorkOrderDetail(' + Data[i].YakkrId + ',this,2)">' + Data[i].workorderSequence + '</td>';
                    tbody = tbody + '<td>' + Data[i].YakkrCode + ' & ' + Data[i].yakkrdescription + '</td><td>' + Data[i].StaffName + '</td><td>' + Data[i].RequestedDate + '</td> <td> ';
                    tbody = tbody + '<a href="/Home/FacilityStaffWorks?YakkrID=' + Data[i].YakkrId + '"  data-yakkrid="' + Data[i].YakkrId + '" data-yakkrdescrp="' + Data[i].yakkrdescription + '" data-yakkr="' + Data[i].YakkrCode + '"  style="text-decoration:underline !important;color:green;">Update Status</a> </td></tr>';
                }

                $('#DivWaitingListPopup > tbody').append(tbody);
                if (!$('#ModalWaitingList').hasClass('in'))
                    $('#ModalWaitingList').modal('toggle');
            }
            else {
                $('#DivSelfAssignedPopup > tbody > tr').remove();
                $('#div5').show();
            }
        }
    </script>


}

