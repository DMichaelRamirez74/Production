                                                @Html.RadioButtonFor(m => m.AddHearing, 1, new { @id = "AddHearing", onchange = "ShowHearing();" })
                                                <span style="font-size: 14px;">Yes</span>
                                            </div>
                                            <div class="col-sm-3">
                                                @Html.RadioButtonFor(m => m.AddHearing, 2, new { @id = "AddHearing1", onchange = "HideHearing();" })
                                                <span style="font-size: 14px;">No</span>
                                            </div>
