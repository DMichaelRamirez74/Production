                                        <label>Allow 30 Day Physical Screening? <sup>*</sup></label>
                                        <div class="cont-sz">
                                            <div class="col-sm-3">
                                                @Html.RadioButtonFor(m => m.AddPhysical, 1, new { @id = "AddPhysical", onchange = "ShowPhysical();" })
                                                <span style="font-size: 14px;">Yes</span>
                                            </div>
                                            <div class="col-sm-3">
                                                @Html.RadioButtonFor(m => m.AddPhysical, 2, new { @id = "AddPhysical1", onchange = "HidePhysical();" })
                                                <span style="font-size: 14px;">No</span>
                                            </div>
                                            <div class="col-sm-4 wellborn-browse">
                                                @Html.TextBoxFor(m => m.Physical, new { @accept = "image/*", @capture = "camera", type = "file", onchange = "ValidateSingleInputpdf2(this)", @class = "form-control", @style = "width:161% !important;" })

                                            </div>
                                            <div class="col-sm-2" style="margin-top: 2%; padding-left: 11%;">
                                                @if (@Model.PhysicalFileName != "")
                                                {
                                                    <a target="_blank" id="Allow30DayPhysical" title="Click here to download" href="~/Agencyuser/getpdfimage/1,@Model.ParentAppID"><i class="fa fa-download"></i></a>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6" id="VisionScreen">
                                    <div class="form-group">
                                        <label>Allow 45 Day Vision Screening? <sup>*</sup></label>
                                        <div class="cont-sz">
                                            <div class="col-sm-3">
                                                @Html.RadioButtonFor(m => m.AddVision, 1, new { @id = "AddVision", onchange = "ShowVision();" })
                                                <span style="font-size: 14px;">Yes</span>
                                            </div>
                                            <div class="col-sm-3">
                                                @Html.RadioButtonFor(m => m.AddVision, 2, new { @id = "AddVision1", onchange = "HideVision();" })
                                                <span style="font-size: 14px;">No</span>
                                            </div>
