                                                    <a target="_blank" id="Allow45DayVisionScreening" title="Click here to download" href="~/Agencyuser/getpdfimage/2,@Model.ParentAppID"><i class="fa fa-download"></i></a>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6" id="HearingScreen">
                                    <div class="form-group">
