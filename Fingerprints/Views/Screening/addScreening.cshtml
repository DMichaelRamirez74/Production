                                        <div class="cont-sz">
                                            <div class="col-sm-3">
                                                @Html.RadioButtonFor(m => m.AddSpeech, 1, new { @id = "AddSpeech", onchange = "ShowSpeech();" })
                                                <span style="font-size: 14px;">Yes</span>
                                            </div>
                                            <div class="col-sm-3">
                                                @Html.RadioButtonFor(m => m.AddSpeech, 2, new { @id = "AddSpeech1", onchange = "HideSpeech();" })
                                                <span style="font-size: 14px;">No</span>
                                            </div>
