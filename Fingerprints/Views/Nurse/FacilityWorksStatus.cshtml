        function AutoComplete(type)
        {
            var parent = "";
            if (type == 1) {
                parent = $('#AssignTo');
            }
            else {
                parent = $('#ReAssignTo');
            }
            var cmpyname = $(parent).find('.CmpyName');
            try {
                $(cmpyname).autocomplete({
                    minLength: 1,
                    source: function (request, response) {
                        $.ajax({
                            url: HostedDir + "/Home/AutoCompleteExternalFacility",
                            type: "POST",
                            dataType: "json",
                            data: { term: request.term },
                            success: function (data) {
                                console.clear();
                                console.log(data);
                                response($.map(data, function (item) {
                                   // $('.CmpyName').val('');
                                    $('.CmpyAddress').val('');
                                    $('.CmpyEmailId').val('');
                                    $('.CmpyContactName').val('');
                                    $('.CmpyContactNo').val('');
                                    return {
                                        label: item.ExternCompanyName, ExternCompanyName: item.ExternCompanyName, ExternalAddress: item.ExternalAddress, ExternalEmailId: item.ExternalEmailId,
                                        ExternalContactName: item.ExternalContactName, ExternalContactNo: item.ExternalContactNo

                                    };
                                }))
                            }

                        })
                    },
                    select: function (event, ui) {
                        $(parent).find('.CmpyName').val(ui.item.ExternCompanyName);
                        $(parent).find('.CmpyAddress').val(ui.item.ExternalAddress);
                        $(parent).find('.CmpyEmailId').val(ui.item.ExternalEmailId);
                        $(parent).find('.CmpyContactName').val(ui.item.ExternalContactName);
                        $(parent).find('.CmpyContactNo').val(ui.item.ExternalContactNo);


                    },
                    messages: {
                        noResults: "No Result Found", results: ""
                    }
                })

            }
            catch (err) {
            }
        }

