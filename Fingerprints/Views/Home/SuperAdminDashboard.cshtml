@{
    @section Title
{
        Dashboard
    }
    Layout = "~/Views/Shared/SuperAdminLayout.cshtml";
}
@section Script{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#spinner').hide();
        $("#side-menu li a").removeClass("active");
        $('#dashboard').addClass("active");
    });
    </script>
}
@section MainContentHolder{
    @{
        Dictionary<string, string> items = ViewBag.superadmindashboard as Dictionary<string, string>;
    }
    <div class="row">
        <div class="col-lg-12">
            <h2 class="page-header">Dashboard</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-4 col-md-6">
            <div class="agencybox1">
                <div class="heading_box1"><h3>Agency Statistics</h3></div>
                <div class="inner-con">
                    <div class="row">
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Total Agencies</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["Totalagency"].ToString() != "0")
                                    {
                                        <a href="~/Agency/viewAgency/1">@items["Totalagency"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["Totalagency"]</a>
                                    }

                                </p>
                            </div>
                        </div>
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Active Agencies</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["Totalactiveagency"].ToString() != "0")
                                    {
                                        <a href="~/Agency/viewAgency/2"> @items["Totalactiveagency"]</a>
                                    }
                                    else
                                    {
                                        <a href="#"> @items["Totalactiveagency"]</a>
                                    }
                                </p>
                            </div>
                        </div>
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Inactive Agencies</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["totaldeactiveagency"].ToString() != "0")
                                    {
                                        <a href="~/Agency/viewAgency/3">@items["totaldeactiveagency"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totaldeactiveagency"]</a>
                                    }
                                </p>
                            </div>
                        </div>

                    </div>
                </div>
            </div><!---agencybox1-->
        </div>
        <div class="col-lg-4 col-md-6">
            <div class="agencybox1">
                <div class="heading_box1"><h3>Staff Statistics</h3></div>
                <div class="inner-con">
                    <div class="row">
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Total Staff</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">

                                    @if (items["totalstaff"].ToString() != "0")
                                    {
                                        <a href="~/SuperAdmin/SuperAdminList/1">@items["totalstaff"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totalstaff"]</a>
                                    }
                                </p>
                            </div>
                        </div>
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Active Staff</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["totalactivestaff"].ToString() != "0")
                                    {
                                        <a href="~/SuperAdmin/SuperAdminList/2">@items["totalactivestaff"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totalactivestaff"]</a>
                                    }
                                </p>
                            </div>
                        </div>

                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Inactive Staff</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["totaldeactivestaff"].ToString() != "0")
                                    {
                                        <a href="~/SuperAdmin/SuperAdminList/3">@items["totaldeactivestaff"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totaldeactivestaff"]</a>
                                    }
                                </p>
                            </div>
                        </div>


                    </div>
                </div>
            </div><!---agencybox1-->
        </div>
        <div class="col-lg-4 col-md-6">
            <div class="agencybox1">
                <div class="heading_box1"><h3>Agency User Statistics</h3></div>
                <div class="inner-con">
                    <div class="row">
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Total Users</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["totalagencyuser"].ToString() != "0")
                                    {
                                        <a href="~/Agency/viewagencyuser/1">@items["totalagencyuser"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totalagencyuser"]</a>
                                    }
                                </p>
                            </div>
                        </div>
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Active Users</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["totalactiveagencyuser"].ToString() != "0")
                                    {
                                        <a href="~/Agency/viewagencyuser/2">@items["totalactiveagencyuser"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totalactiveagencyuser"]</a>
                                    }
                                </p>
                            </div>
                        </div>

                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Inactive Users</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["totaldeactiveagencyuser"].ToString() != "0")
                                    {
                                        <a href="~/Agency/viewagencyuser/3">@items["totaldeactiveagencyuser"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totaldeactiveagencyuser"]</a>
                                    }
                                </p>
                            </div>
                        </div>
                        <div class="wrp-txt">
                            <div class="col-sm-9 col-xs-9"><p>Suspended Users</p></div>
                            <div class="col-sm-3 col-xs-3">
                                <p style="text-align:right;">
                                    @if (items["totalsuspendedagencyuser"].ToString() != "0")
                                    {
                                        <a href="~/Agency/viewagencyuser/4">@items["totalsuspendedagencyuser"]</a>
                                    }
                                    else
                                    {
                                        <a href="#">@items["totalsuspendedagencyuser"]</a>
                                    }
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!---agencybox1-->
        </div>


    </div>
   
}

