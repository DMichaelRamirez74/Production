@model FingerprintsModel.Screening
<script type="text/javascript">
    $(document).ready(function () {
        $('input').keypress(function () {
            if ($(this).val() == 'MM/DD/YYYY') {
                $(this).val('');
                // $(this).mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
            }
        });

        $('#E060denDate').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        $('#E063denReferralDate').mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
        if ('@Model.E062denPrevent' == "")
            $('#E062denPrevent').val("");
        if ('@Model.E061denResults' == "")
            $('#E061denResults').val("");
        if ('@Model.E064denTreatment' == "")
            $('#E064denTreatment').val("");
        if ('@Model.E066denTreatmentReceive' == "")
            $('#E066denTreatmentReceive').val("");


    });
</script>
<div class="row add-useragform1">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 add-useragform">

        <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">

            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Dental Screening Date  <sup>*</sup></label>
                    <div class="date custom-time">
                        <input class="form-control" id="E060denDate" maxlength="50" name="E060denDate" onblur="return validatedate1(this);" placeholder="MM/DD/YYYY" type="text" value="@Model.E060denDate" @if (!string.IsNullOrEmpty(Model.E060denDate)){ <text> readonly="readonly" </text>   } />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Dental Screening Results</label>
                    <select name="E061denResults" class="form-control" id="E061denResults">
                        @*<option value="" @if (@Model.E061denResults == "") { <text> selected="selected" </text>   }></option>*@
                        <option value="1" @if (@Model.E061denResults == "1") { <text> selected="selected" </text>               }>Complete</option>
                        <option value="2" @if (@Model.E061denResults == "2") { <text> selected="selected" </text>               }>Treatment Needed</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Dental Preventative Completed</label>

                    <select name="E062denPrevent" class="form-control" id="E062denPrevent">
                        @*<option value="" @if (@Model.E062denPrevent == "") { <text> selected="selected" </text>   }></option>*@
                        <option value="1" @if (@Model.E062denPrevent == "1") { <text> selected="selected" </text>               }>Yes</option>
                        <option value="2" @if (@Model.E062denPrevent == "2") { <text> selected="selected" </text>               }>No</option>
                    </select>
                </div>
            </div>

            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group datepicker-days">
                    <label>Dental Referral Date</label>
                    <div class="date custom-time">
                        <input class="form-control" id="E063denReferralDate" maxlength="50" name="E063denReferralDate" onblur="return validatedate1(this);" placeholder="MM/DD/YYYY" type="text" value="@Model.E063denReferralDate" />
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Dental Treatment</label>

                    <select name="E064denTreatment" class="form-control" id="E064denTreatment">
                        @*<option value="" @if (@Model.E064denTreatment == "") { <text> selected="selected" </text>   }></option>*@
                        <option value="1" @if (@Model.E064denTreatment == "1") { <text> selected="selected" </text>               }>No Treatment Needed</option>
                        <option value="2" @if (@Model.E064denTreatment == "2") { <text> selected="selected" </text>               }>Treatment Needed, Not Received</option>
                        <option value="3" @if (@Model.E064denTreatment == "3") { <text> selected="selected" </text>               }>Treatment Needed, Received</option>
                        <option value="4" @if (@Model.E064denTreatment == "4") { <text> selected="selected" </text>               }>Treatment Received, Further Treatment Needed</option>
                        <option value="5" @if (@Model.E064denTreatment == "5") { <text> selected="selected" </text>               }>Treatment Received, Refusal Signed</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Dental Treatment Comments</label>
                    <div class="date custom-time">
                        <input class="form-control" id="E065denTreatmentComments" maxlength="100" name="E065denTreatmentComments" type="text" value="@Model.E065denTreatmentComments" />
                    </div>
                </div>
            </div>

            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <label>Has the child received dental treatment at any time this year? </label>

                    <select name="E066denTreatmentReceive" class="form-control" id="E066denTreatmentReceive">
                        @*<option value="" @if (@Model.E066denTreatmentReceive == "") { <text> selected="selected" </text>   }></option>*@
                        <option value="1" @if (@Model.E066denTreatmentReceive == "1") { <text> selected="selected" </text>             }>Yes</option>
                        <option value="2" @if (@Model.E066denTreatmentReceive == "2") { <text> selected="selected" </text>             }>No</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">


            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                <div class="form-group">
                    <label class="custom-file-upload">
                        <input name="ScreeningDocument" id="ScreeningDocument" class="form-control" onchange="ValidateSingleInputpdf(this)" style="display:none" type="file" value="">
                        <i class="fa fa-cloud-upload"></i> Add new document
                    </label>
                    <label style="margin-top: 8px;margin-left: 6px;" id="documentlabel"></label>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">


                @if (!string.IsNullOrEmpty(Model.PhysicalFileName))
                {
                    if (Model.PhysicalFileExtension == ".pdf")
                    {
                        string url = Path.GetFileNameWithoutExtension(Model.PhysicalFileName) + Guid.NewGuid().ToString() + ".pdf";
                        System.IO.FileStream file = System.IO.File.Create(Server.MapPath("~//TempAttachment//") + url);
                        url = "..\\TempAttachment\\" + url;
                        try
                        {
                            file.Write(Model.PhysicalImageByte, 0, Model.PhysicalImageByte.Length);
                            file.Close();
                        }
                        catch (Exception Ex)
                        {
                            FingerprintsModel.clsError.WriteException(Ex);
                        }
                        finally
                        {
                            file.Close();
                        }
                        <div style="display:none;" id="ScreeninguploadDocument">
                        </div>
                        <iframe class="iframestyle" src="@url" frameborder="0" scrolling="no"></iframe>
                    }
                    else
                    {
                        string imagedata = Convert.ToBase64String(Model.PhysicalImageByte);
                        <div id="ScreeninguploadDocument">
                            <script type="text/javascript">

                                $(document).ready(function () {
                                    $('#ScreeninguploadDocument').ZoomCrop(
                                  {
                                      image: "data:image/png;base64," + '@imagedata',
                                      updated: function (size, crop, position) {
                                          console.log(size, crop, position);
                                      }
                                  });
                                });
                            </script>

                        </div>

                    }
                }
                else
                {
                    <div id="ScreeninguploadDocument">
                        <script type="text/javascript">

                            $(document).ready(function () {
                                $('#ScreeninguploadDocument').ZoomCrop(
                              {
                                  image: HostedDir + "/Content/img/NoImageAvailable.jpg",
                                  updated: function (size, crop, position) {
                                      console.log(size, crop, position);
                                  }
                              });
                            });
                        </script>
                    </div>

                }



            </div>

            </div>

    </div>
</div>