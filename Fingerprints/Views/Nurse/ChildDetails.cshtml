            if ((isNaN(inputTextObject.value)) && (len < 5)) {
                $(inputTextObject).val('');
                customAlert("Zip code must be numeric. ");
                var ret = false;
            }
            else if (len < 5) {
                $(inputTextObject).val('');
                customAlert("Zip code length must be 5 digit. ");
                var ret = false;
            }
            else if (isNaN(inputTextObject.value)) {
                $(inputTextObject).val('');
                customAlert("Zip code must be numeric. ");
                var ret = false;
            }
            return ret;
        }
    }

