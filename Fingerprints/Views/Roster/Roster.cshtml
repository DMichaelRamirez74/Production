            var rowLength = $modal.find('#table_body').find('tr').length;
            if (rowLength === 0) {

                return false;
            }



            $('#spinner').show();

            window.setTimeout(function () {

                pageLoadedFirst = false;
                pageSize_caseNote = $modal.find('#casenoteddlpagetodisplay').val();
                if (val == 'First') {
                    StartIndex = 0;
                    LastIndex = parseInt(pageSize_caseNote) + parseInt(LastIndex * requestedPage_caseNote);
                    requestedPage_caseNote = ((StartIndex / 10) + 1);

                    $modal.find('#First_CaseNote').attr('disabled', true);
                    $modal.find('#Back_CaseNote').attr('disabled', true);
                    $modal.find('#Next_CaseNote').attr('disabled', false);
                    $modal.find('#Last_CaseNote').attr('disabled', false);
                    LastIndex = 0;
                }
                else if (val == 'Last') {
                    StartIndex = parseInt((totalRecords_caseNote - 1) / pageSize_caseNote) * pageSize_caseNote;
                    LastIndex = totalRecords_caseNote;
                    requestedPage = numOfPages_caseNote;
                    // GoToNextPage(requestedPage, pageSize_caseNote)
                    $modal.find('#First_CaseNote').attr('disabled', false);
                    $modal.find('#Back_CaseNote').attr('disabled', false);
                    $modal.find('#Next_CaseNote').attr('disabled', true);
                    $modal.find('#Last_CaseNote').attr('disabled', true);
                }
                else if (val == 'Next') {
                    LastIndex = parseInt(pageSize_caseNote) + parseInt(LastIndex);
                    requestedPage_caseNote = (parseInt(LastIndex / pageSize_caseNote) + 1);
                    //   GoToNextPage(requestedPage_caseNote, pageSize_caseNote);

                    $modal.find('#First_CaseNote').attr('disabled', false);
                    $modal.find('#Back_CaseNote').attr('disabled', false);
                    if (parseInt(LastIndex) + parseInt(pageSize_caseNote) >= totalRecords_caseNote) {
                        $modal.find('#Next_CaseNote').attr('disabled', true);
                        $modal.find('#Last_CaseNote').attr('disabled', true);
                    }
                    else if (parseInt(LastIndex) - parseInt(pageSize_caseNote) < totalRecords_caseNote) {
                        $modal.find('#Next_CaseNote').attr('disabled', false);
                        $modal.find('#Next_CaseNote').attr('disabled', false);
                    }
                }
                else if (val == 'Back') {
                    requestedPage_caseNote = requestedPage_caseNote - 1;
                    LastIndex = parseInt(LastIndex) - parseInt(pageSize_caseNote);
                    // GoToNextPage(requestedPage_caseNote, pageSize_caseNote)
                    if (parseInt(LastIndex) + parseInt(pageSize_caseNote) > totalRecords_caseNote) {
                        $modal.find('#Next_CaseNote').attr('disabled', true);
                        $modal.find('#Next_CaseNote').attr('disabled', true);
                    }
                    else if (parseInt(LastIndex) - parseInt(pageSize_caseNote) < totalRecords_caseNote) {
                        $modal.find('#Next_CaseNote').attr('disabled', false);
                        $modal.find('#Next_CaseNote').attr('disabled', false);
                    }
                    if (requestedPage_caseNote == 1) {
                        $modal.find('#First_CaseNote').attr('disabled', true);
                        $modal.find('#Back_CaseNote').attr('disabled', true);
                    }
                }
                else {
                }

                GoToNextPage_CaseNote(requestedPage_caseNote, pageSize_caseNote);

            }, 10)
        }





        function getTotalRecord(data) {
            $('#First').attr('disabled', false);
            $('#Back').attr('disabled', false);
            $('#Next').attr('disabled', false);
            $('#Last').attr('disabled', false);
            pageSize = $('#ddlpagetodisplay').val();
            if (data > 0) {
                totalRecords = parseInt(data);
                if (totalRecords <= pageSize) {
                    $('#First').attr('disabled', true);
                    $('#Back').attr('disabled', true);
                    $('#Next').attr('disabled', true);
                    $('#Last').attr('disabled', true);
                }
                numOfPages = parseInt(totalRecords / pageSize) + ((totalRecords % pageSize == 0) ? 0 : 1);
                $("#ddlpaging").empty()
                for (i = 1; i <= numOfPages; i++) {
                    var newOption = "<option value='" + i + "'>" + i + "</option>";
                    $("#ddlpaging").append(newOption);
                }
                $("#ddlpaging").val(requestedPage);
            }
            else {
                $('#First').attr('disabled', true);
                $('#Back').attr('disabled', true);
                $('#Next').attr('disabled', true);
                $('#Last').attr('disabled', true);
            }
        }




