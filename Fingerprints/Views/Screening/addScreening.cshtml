                                                </div>
                                                <div class="col-sm-2" style="margin-top: 2%; padding-left: 11%;">
                                                    <a target="_blank" id="Allow45DayDevelopmentalScreening" title="Click here to download" href="~/Agencyuser/getpdfimage/4,@Model.ParentAppID"><i class="fa fa-download"></i></a>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                                <div class="col-lg-6" id="DentalScreen">
                                    <div class="form-group">
                                        <label>Allow 90 Day Dental Screening? <sup>*</sup></label>
                                        <div class="cont-sz">
