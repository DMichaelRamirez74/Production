@model FingerprintsModel.Screening
@using System.Data


<input type="hidden" id="viewBagText" value="@ViewBag.Title"/>
 
<div class="row" id="ScreeningDiv">

    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">

        <div class="panel with-nav-tabs panel-primary panelscreen">
            <div class="panel-heading panelul">

                <ul id="myTab" class="nav nav-tabs navtabs1">
                    @{

                        try
                        {
                            string TabName = "";
                            string Tabid = "";
                            if (@ViewBag.tabpageScreening == "1")
                            {

                                <li id="Screeningallowed"><a href="#AllowScreening" data-toggle="tab">Parent Approval</a></li>
                                <li style="display:none;" id="Physical" class="active"><a href="#addPhysical" data-toggle="tab">30 Day Physical</a></li>
                                <li style="display:none;" id="Vision"><a href="#addVision" data-toggle="tab">45 Day Vision</a></li>
                                <li style="display:none;" id="Hearing"><a href="#addHearing" data-toggle="tab">45 Day Hearing</a></li>
                                <li style="display:none;" id="Develop"> <a href="#addDevelop" data-toggle="tab">45 Day Developmental</a></li>
                                <li style="display:none;" id="Dental"><a href="#addDental" data-toggle="tab">90 Day Dental</a></li>
                                <li style="display:none;" id="Speech"><a href="#addSpeech" data-toggle="tab">Speech Screening</a></li>
                                if (Model.CustomScreening != null && Model.CustomScreening.Rows.Count > 0)
                                {
                                    DataTable ScreeningNames = Model.CustomScreening.DefaultView.ToTable(true, "ScreeningName", "programtype");
                                    foreach (DataRow Row in ScreeningNames.Rows)
                                    {

                                        TabName = "#add" + @Row["ScreeningName"].ToString();
                                        Tabid = @Row["ScreeningName"].ToString() + "tabid";
                                        <li style="display:none;" programtype="@Row["programtype"].ToString()" class="customtabhide" id="@Tabid"><a href="@TabName" data-toggle="tab">@Row["ScreeningName"].ToString()</a></li>
                                    }
                                }
                            }
                            else
                            {
                                <li id="Screeningallowed" class="active"><a href="#AllowScreening" data-toggle="tab">Parent Approval</a></li>
                                        <li style="display:none;" id="Physical"><a href="#addPhysical" data-toggle="tab">30 Day Physical</a></li>
                                        <li style="display:none;" id="Vision"><a href="#addVision" data-toggle="tab">45 Day Vision</a></li>
                                        <li style="display:none;" id="Hearing"><a href="#addHearing" data-toggle="tab">45 Day Hearing</a></li>
                                        <li style="display:none;" id="Develop"> <a href="#addDevelop" data-toggle="tab">45 Day Developmental</a></li>
                                        <li style="display:none;" id="Dental"><a href="#addDental" data-toggle="tab">90 Day Dental</a></li>
                                        <li style="display:none;" id="Speech"><a href="#addSpeech" data-toggle="tab">Speech Screening</a></li>
                                if (Model.CustomScreening != null && Model.CustomScreening.Rows.Count > 0)
                                {
                                    DataTable ScreeningNames = Model.CustomScreening.DefaultView.ToTable(true, "ScreeningName", "programtype");
                                    foreach (DataRow Row in ScreeningNames.Rows)
                                    {

                                        TabName = "#add" + @Row["ScreeningName"].ToString();
                                        Tabid = @Row["ScreeningName"].ToString() + "tabid";
                                        <li style="display:none;" programtype="@Row["programtype"].ToString()" class="customtabhide" id="@Tabid"><a href="@TabName" data-toggle="tab">@Row["ScreeningName"].ToString()</a></li>
                                    }
                                }
                            }

                        }
                        catch (Exception Ex)
                        {
                            FingerprintsModel.clsError.WriteException(Ex);
                        }

                    }
                </ul>
            </div>
            <div id="myTabContent" class="tab-content">

                <div class="tab-pane fade in active" id="AllowScreening">
                    <div class="add-useragform">
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 table-main">
