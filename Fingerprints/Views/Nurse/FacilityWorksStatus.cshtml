            $(parent).find('.spnDescription').text(data.UserDescription);
            $(parent).find('.spnCenterName').text(data.CenterName);
            $(parent).find('.spnStaff').text(data.StaffName);
            $(parent).find('.spnFixedType').text(data.IsTemporaryFix);
        //    $(parent).find('.spnAssignTo').text(data.AssignedTo);
            $(parent).find('.spnDate').text(data.RequestedDate);



        }

        function BindWorkList(Data,parent)
        {
            debugger;

            if (Data.workOrderList != null && Data.workOrderList.length > 0) {

                for (var j = 0; j < Data.workOrderList.length; j++) {
                    DivContent = "", tabContent = "", tabRow = "";;
                    DivContent = $('#GetAppendDiv').html();
                    DivContent = DivContent.replace('SectionTitle', "Fix Number " + (j + 1))
                    DivContent = DivContent.replace('#collapse13', "#collapse" + (j + 1))
                    DivContent = DivContent.replace('collapse13', "collapse" + (j + 1))
                    DivContent = DivContent.replace('StaffWorked', Data.workOrderList[j].StaffName);
                    DivContent = DivContent.replace('CurrentStatus', Data.workOrderList[j].IsTemporaryFix);

                      if (Data.workOrderList[j].ImageCount == 0)
                        DivContent = DivContent.replace('ImageCount', '<a href="#" >' + Data.workOrderList[j].ImageCount + '</a>');
                      else
                        DivContent = DivContent.replace('ImageCount', '<a href="#" style="text-decoration:underline;" onclick="LoadCarouselImages(' + Data.workOrderList[j].WorkId + ')">' + Data.workOrderList[j].ImageCount + '</a>');

                    DivContent = DivContent.replace('Notes123', Data.workOrderList[j].Notes);
                    DivContent = DivContent.replace('DateGiven', Data.workOrderList[j].RequestedDate);

                    var tr = "<tbody>";
                    if (Data.workOrderList[j].PartDetails != null && Data.workOrderList[j].PartDetails.length > 0) {

                        for (var i = 0; i < Data.workOrderList[j].PartDetails.length; i++) {

                            tr += "<tr>";
                            tr += '<td>' + (j + 1) + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].PartNumber + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].PartDescription + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].Quantity + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].UnitCost + '</td>';
                            tr += '<td>' + Data.workOrderList[j].PartDetails[i].TotalCost + '</td>';
                            tr += "</tr>";
                        }
                        DivContent = DivContent.replace("<tbody>", tr);
                    }
                    else {

                        tr += "<tr>";
                        tr += "<td colspan='6' style=' text-align: center; color: #db4437;'>No record found</td>";
                        tr += "</tr>";
                        DivContent = DivContent.replace("<tbody>", tr);
                    }
                    $(parent).find('.workparts').append(DivContent);
                }



            }
            else {

            }

        }
