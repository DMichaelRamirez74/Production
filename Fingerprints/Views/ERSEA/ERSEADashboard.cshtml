                margin-top: 12px;
            }
            .ersea-table-head p
            {
                text-align: center;
            }
            .total_title
            {
                text-align: left!important;
            }
            #no-more-tables8 tr {
    padding-bottom: 20px;
    box-shadow: 0px 0px 5px #e8d8d8;
        background: #f1f5f8!important;
}         

            .ersea-table
            {
                background: white!important;
            }
            #no-more-tables8 td
            {
                background: white;
            }

            #no-more-tables8 td {
    min-height: 35px;
}
        }

        tbody.ersea-table-head tr td
        {
            background: transparent!important;
        }

                tbody.ersea-table-head tr td p
        {
            background: transparent!important;
        }

                    .ersea-table-head tr:nth-of-type(odd) {
    background-color: #f9f9f9;
}
            
            .ersea-table-head tr:nth-of-type(even) {
    background-color: #f1f5f8;
}
        .head-width-cal {
         width:calc( 100% - 17px ) !important;
        }

