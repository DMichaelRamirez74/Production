@model FingerprintsModel.CaseNoteByClientID



<style type="text/css">
    #no-more-tables7 table thead > tr > th {
        padding: 15px 5px 15px 5px;
    }
</style>

<div id="no-more-tables7" class="col-xs-12 no-padding" style="margin-bottom: 20px;">

    <div class="col-xs-12 search-block-main" style="padding-bottom:5px;display:block; ">

        <div class="col-lg-3" style="float:right;text-align:right;">


            <table class="col-md-12 table-striped total-details-table table-back-shadow">
                <tbody>
                    <tr style="background-color: #f1f5f8;">
                        <td><label>Total Record(s)</label></td>
                        <td id="totalCountSpan_casenote">@Model.TotalRecord</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <div class="col-xs-12">



        <table class="table table-striped table-condensed glossy-table">
            <thead>
                <tr>

                    <th>

                        @if (Model!=null && Model.SortColumn!=null && Model.SortColumn.ToLowerInvariant() == "th1")
                        {

                            if (Model.SortOrder.ToLowerInvariant() == "desc")
                            {
                                <i class="fa fa-custom-sort-date-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                                <i class="fa fa-custom-sort-date-desc i-desc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                            }
                            else if (Model.SortOrder.ToLowerInvariant() == "asc")
                            {
                                <i class="fa fa-custom-sort-date-asc i-asc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                                <i class="fa fa-custom-sort-date-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            }


                        }
                        else
                        {
                            <i class="fa fa-custom-sort-date-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            <i class="fa fa-custom-sort-date-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                        }


                        <span col-name="th1">Date</span>
                    </th>



                    <th style="width:50%">

                        @if (Model!=null && Model.SortColumn!=null && Model.SortColumn.ToLowerInvariant() == "th2")
                        {

                            if (Model.SortOrder.ToLowerInvariant() == "desc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                            }
                            else if (Model.SortOrder.ToLowerInvariant() == "asc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            }


                        }
                        else
                        {
                            <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                        }


                        <span col-name="th2">Title</span>
                    </th>

                    <th style="width:53%">

                        @if (Model!=null && Model.SortColumn!=null && Model.SortColumn.ToLowerInvariant() == "th3")
                        {

                            if (Model.SortOrder.ToLowerInvariant() == "desc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                            }
                            else if (Model.SortOrder.ToLowerInvariant() == "asc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            }


                        }
                        else
                        {
                            <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                        }


                        <span col-name="th3">By</span>
                    </th>

                    <th width="4%;">

                        @if (Model!=null && Model.SortColumn!=null && Model.SortColumn.ToLowerInvariant() == "th4")
                        {

                            if (Model.SortOrder.ToLowerInvariant() == "desc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                            }
                            else if (Model.SortOrder.ToLowerInvariant() == "asc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            }


                        }
                        else
                        {
                            <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                        }


                        <span col-name="th4">Attachments</span>
                    </th>

                    <th width="1%;">

                        @if (Model!=null && Model.SortColumn!=null &&  Model.SortColumn.ToLowerInvariant() == "th5")
                        {

                            if (Model.SortOrder.ToLowerInvariant() == "desc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                            }
                            else if (Model.SortOrder.ToLowerInvariant() == "asc")
                            {
                                <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:inline-block;"></i>
                                <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            }


                        }
                        else
                        {
                            <i class="fa fa-sort-alpha-asc i-asc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                            <i class="fa fa-sort-alpha-desc i-desc" aria-hidden="true" style="color: #f9c751;display:none;"></i>
                        }


                        <span col-name="th5">References</span>
                    </th>

                    <th>Action</th>
                </tr>
            </thead>
            <tbody class="glossy-table-body">
                @{ 
                    Html.RenderPartial("~/Views/Partialviews/_CaseNoteListPartial.cshtml", Model);

                }
               
            </tbody>
        </table>
    </div>
</div>

<div class="col-xs-12" id="div-pagination-casenote" style="margin-top:20px;">
    
</div>

<script>
    $('[data-toggle="tooltip"]').tooltip();

    $(function () {

  
    })

</script>

