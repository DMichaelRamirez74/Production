        function AddQuestions() {
            $('#div1').hide();
            var Question = "CustomQues[" + rounds + "].Question";
            var QuesYes = "CustomQues[" + rounds + "].QuesYes";
            var QuesNo = "CustomQues[" + rounds + "].QuesNo";
          //  var notesP = "ParentPhone1[" + rounds + "].notesP";
           // var SmsP = "ParentPhone1[" + rounds + "].SmsP";
            var tr = '<tr><td ><input style="width:100%;" class = "form-control" name=' + Question + ' type="text"  ></td><td ><input style="width:100%;" onblur="checkIfNumericDigit(this);" class = "form-control" name=' + QuesYes + ' type="text"  ></td><td ><input style="width:98%;" class = "form-control" onblur="checkIfNumericDigit(this);" name=' + QuesNo + ' type="text"  ></td><td><a href="javascript:void(0)" onclick="RemoveQues(this);" style=" text-decoration:underline;">Delete</a></td></tr>';
            $('#gridquestion > tbody').append(tr);
          
            $('#gridquestion > tbody tr:last').find('td:eq(0) :input').val("");
            rounds = rounds + 1;
        }
        function RemoveQues(value) {
            if ($(value).attr("accesskey")) {
                //debugger;
               // if ($('#gridquestion > tbody tr').length > 1) {
                    BootstrapDialog.confirm('Do you want to delete this question?', function (result) {
                        if (result) {
                            $.ajax({
                                url: HostedDir + "/AgencyAdmin/Deletecustomques",
                                type: "POST",
                                data: {
                                    CQID: $(value).attr("accesskey"),
                                   
                                },
                                dataType: "json",
                                secureuri: false,
                                async: false,
                                success: function (response) {

                                    if (response == 1) {
                                        customAlert("Question deleted successfully. ");
                                        rounds = 0;
                                        //debugger;
                                        if ($('#gridquestion > tbody tr').length == 1) {
                                            $(value).closest('tr').remove();
                                            $('#div1').show();
                                        }
                                        if ($('#gridquestion > tbody tr').length > 1) {

                                            $(value).closest('tr').remove();
                                            
                                        }
                                        $('#gridquestion > tbody tr').each(function () {
                                            var Question = "CustomQues[" + rounds + "].Question";
                                            var QuesYes = "CustomQues[" + rounds + "].QuesYes";
                                            var QuesNo = "CustomQues[" + rounds + "].QuesNo";
                                            var CQID = "CustomQues[" + round + "].CQID";
                                            $(this).find('td:eq(0) :input[type="text"]').attr('name', Question);
                                            $(this).find('td:eq(1) :input[type="text"]').attr('name', QuesYes);
                                            $(this).find('td:eq(2) :input[type="text"]').attr('name', QuesNo);
                                            $(this).find('td:eq(3)').find(':input').attr('name', CQID);
                                            rounds = rounds + 1;
                                        });
                                    }
                                }
                            , error: function (response) {
                                   customAlert("Session Ended Log Onto The System Again.");setTimeout(function () {window.location.href= HostedDir + '/login/Loginagency';   }, 2000);
                            }
                            });
                        }
                    });
                //}

            }
            else {
                rounds = 0;
                if ($('#gridquestion > tbody tr').length > 1) {
                    $(value).closest('tr').remove();
                }
                $('#gridquestion > tbody tr').each(function () {
