                                                            <th style="text-align: center;">Enrollment (@Model.OverAllEnrollment)</th>
                                                            <th style="text-align: center;">Withdrawn (@Model.OverAllWithdrawn)</th>
                                                            <th style="text-align: center;">Dropped (@Model.OverAllDropped)</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="ersea-table-head-enroll scroll-tbody">
                                                        @foreach (FingerprintsModel.ApplicationEnrollment obj in Model.lstApplication)
                                                        {
                                                            <tr>
                                                                <td data-title="Zipcode">
                                                                    <p style="text-align: center;">@obj.ZipCode</p>
                                                                </td>
                                                                <td data-title="City Name">
                                                                    <p style="text-align: center;">@obj.CityName</p>
                                                                </td>
                                                                <td data-title="Application (@Model.OverAllApplication)">
                                                                    <p style="text-align: center;">@obj.Application</p>
                                                                </td>
                                                                <td data-title="Enrollment (@Model.OverAllEnrollment)">
                                                                    <p style="text-align: center;">@obj.Enrollment</p>
                                                                </td>
                                                                <td data-title="Withdrawn (@Model.OverAllWithdrawn)">
                                                                    <p style="text-align: center;">@obj.Withdrawn</p>
                                                                </td>
                                                                <td data-title="Dropped (@Model.OverAllDropped)">
                                                                    <p style="text-align: center;">@obj.Dropped</p>
                                                                </td>

                                                            </tr>
                                                        }



                                                    </tbody>
                                                </table>
                                            </div>

                                            <div class="col-xs-12 poppage-slot" style="padding:0px;display:none;">
                                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 pagination-file">
                                                    <div class="pages_display">
                                                        <ul>
                                                            <li style="color:#fff;">Display</li>
                                                            <li>
                                                                <select id="ddlpagetodisplay" onchange="drawgrid('#FosterDisplaymodal',this);" center="NzU=">
                                                                    <option value="10" selected="selected">10</option>
                                                                    <option value="25">25</option>
                                                                    <option value="50">50</option>
                                                                    <option value="100">100</option>
                                                                </select>
                                                            </li>
                                                            <li style="color:#fff;">Records Per Page</li>
                                                        </ul>
                                                        <div class="clear"></div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                    <div id="divPaging" class="pagination_wrp">
                                                        <ul id="ulPaging" class="pagination">
                                                            <li class="pagination-ch"><i class="fa fa-angle-double-left"></i></li>
                                                            <li class="pagination-ch" title="Back"><i class="fa fa-angle-left"></i></li>
                                                            <li title="Select">
                                                                <select class="select_cl" style="top: 0;" id="ddlpaging" onchange="getListafterupdation('#FosterDisplaymodal', this)" center="NzU="><option value="1">1</option><option value="2">2</option></select>
                                                            </li>
                                                            <li class="pagination-ch" title="Next"><i class="fa fa-angle-right"></i></li>
                                                            <li class="pagination-ch" title="Last"><i class="fa fa-angle-double-right"></i></li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>



                                    </div>

                                </div>

                            </div>

                            <div id="ada" class="tab-pane fade">

                                <div class="privacy_lst">
                                    <div class="notifi_lst">

                                        <div id="no-more-tables8" class="col-xs-12 no-padding" style="margin-bottom: 20px;">
                                           
                                            
                                             <div class="col-xs-12" style="border-radius: 10px;background: #f1f5f8;padding: 10px;width: 98%;margin:20px 1%;">
                                               
                                            @{
                                                Html.RenderPartial("~/Views/ERSEA/_ADAByCenterPartial.cshtml", Model);
                                            }
                                             </div>


                                        </div>




                                    </div>
                                </div>

                            </div>








                        </div>



                    </div>
                    <!---------------------------------------------->

                </div>
            </div>
        </div>
    </div>

    <script type="text/javascript">
        $(document).ready(function () {
            var linkys = document.getElementsByClassName('dropdown-toggle');
            for (var i = 0; i < linkys.length; ++i) {
                linkys[i].onclick = function (e) {
                    this.focus();
                    e.preventDefault();
                };
            }


            $('p[data-toggle="tooltip"]').tooltip();
           
        });

    </script>
