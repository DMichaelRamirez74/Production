                    $('#OtherLanguage12').hide();
                }

            });

            if ('@TempData["message"]' != '') {
                customAlert('@TempData["message"]');
            }
            if ('@Session["Roleid"]' != "2d9822cd-85a3-4269-9609-9aabb914d792" && '@Session["Roleid"]' != "a65bb7c2-e320-42a2-aed4-409a321c08a5") {

