                                        <label for="email">Guardian</label>
                                        @Html.TextBoxFor(m => m.Parentname, new { id = "Parentscreening", maxlength = "50", @class = "form-control", tabIndex = "2" })
                                    </div>
                                </div>
                                <div class="col-lg-6" id="PhysicalScreen">
