                isAllow = true;

                $('#Days').val(selectedDate.getDay());
            } else {
                isAllow = false;
            }

            return isAllow;
        }

        function Checkdate(date) {
            var isAllow = false;
            var now = new Date();
            var selectedDate = new Date(date);
            if (selectedDate < now && selectedDate != now) {
