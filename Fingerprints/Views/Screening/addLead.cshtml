            customAlert("Please enter first name.");
            plainValidation('#txtFname');
            return isValid;
        }
        if ($('#txtLname')[0].value.trim() == "") {
            isValid = false;
            customAlert("Please enter last name.");
            plainValidation('#txtLname');
            return isValid;
        }
