<div class="modal fade scroll-modal custom-modal" id="modal-email-report" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-ch">
        <div class="modal-content">

            <div class="modal-body">
                <button type="button" class="close" data-dismiss="modal"><img src="/Content/CaseNote/images/close.png"></button>
                <h2 id="CaseNoteHeading" class="extra-title muted">@Fingerprints.LocalResource.Resources.EmailReport </h2>
                <form id="formEditInkind" enctype="multipart/form-data">

                    <div class="max_items_addn popup-addn-scroll">

                        <div class="form-group col-xs-12">
                            <label for="lbl-popup-center" class="col-md-4 col-lg-2 col-xs-12" style="color:#295b8f;">@Fingerprints.LocalResource.Resources.Center</label>
                            <label id="lbl-popup-center" class="col-md-5 col-lg-8 col-xs-12" style="font-weight:normal;"></label>
                        </div>

                        <div class="form-group col-xs-12">
                            <label for="lbl-popup-classroom" class="col-md-4 col-lg-2 col-xs-12" style="color:#295b8f;">@Fingerprints.LocalResource.Resources.Classroom</label>
                            <label id="lbl-popup-classroom" class="col-md-5 col-lg-8 col-xs-12" style="font-weight:normal;"></label>
                        </div>
                        <div class="form-group col-xs-12">
                            <label for="lbl-popup-classdate" class="col-md-4 col-lg-2 col-xs-12" style="color:#295b8f;">@Fingerprints.LocalResource.Resources.ClassDate</label>
                            <label id="lbl-popup-classdate" class="col-md-5 col-lg-8 col-xs-12" style="font-weight:normal;"></label>
                        </div>










                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="padding-top:20px;">
                            <div class="panel with-nav-tabs panel-primary col-xs-12" style="padding:0;">
                                <div class="panel-heading panelul">
                                    <ul id="myTab" class="nav nav-tabs navtabs1">
                                        <li class="active"><a href="#Emailsent" data-toggle="tab" aria-expanded="true"><span>@Fingerprints.LocalResource.Resources.EmailsSent</span>&nbsp;<span class="badge badge-success sent-badge">0</span> </a></li>
                                        <li>
                                            <a href="#Bouncebackemails" data-toggle="tab">
                                                <span>@*Bounced Emails*@ @Fingerprints.LocalResource.Resources.UnsentEmails</span>&nbsp;<span class="badge badge-warning bounced-badge">0</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#Noemails" data-toggle="tab">
                                                <span>@Fingerprints.LocalResource.Resources.NoEmails</span>&nbsp;<span class="badge badge-danger no-email-badge">0</span>
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                                <div id="myTabContent" class="tab-content">
                                    <div class="tab-pane fade in active" id="Emailsent">

                                        <div class="col-xs-12 table-scroll-div" style="padding:15px;">
                                            <div class="col-xs-12 text-left" style="margin-bottom:10px;font-size:12px;padding:0;">
                                                <label>
                                                    <span style="color:rgb(63, 81, 181);margin-right: 10px;">H - @Fingerprints.LocalResource.Resources.HomePhone</span>
                                                    &nbsp;<span style="color:rgb(0, 150, 136);margin-right: 10px;">M - @Fingerprints.LocalResource.Resources.MobilePhone</span>
                                                    &nbsp;<span style="color:rgb(121, 85, 72);">W - @Fingerprints.LocalResource.Resources.WorkPhone</span>
                                                </label>
                                            </div>
                                            <table class="col-md-12 case-note-table glossy-table table-striped table-condensed cf table-change ersea-table">
                                                <thead class="table-hd scroll-thead  thead">
                                                    <tr>
                                                        <th>@Fingerprints.LocalResource.Resources.Client</th>
                                                        <th>@Fingerprints.LocalResource.Resources.ParentName</th>
                                                        <th>@Fingerprints.LocalResource.Resources.PhoneNumber</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="ersea-table-head glossy-table-body bind_body scroll-tbody" id="tbody-emailsent"></tbody>
                                            </table>
                                        </div>


                                    </div>

                                    <div class="tab-pane fade" id="Bouncebackemails">
                                        <div class="col-xs-12" style="padding:15px;">

                                            <div class="col-xs-12 text-left" style="margin-bottom:10px;font-size:12px;padding:0;">
                                                <label>
                                                    <span style="color:rgb(63, 81, 181);margin-right: 10px;">H - @Fingerprints.LocalResource.Resources.HomePhone</span>
                                                    &nbsp;<span style="color:rgb(0, 150, 136);margin-right: 10px;">M - @Fingerprints.LocalResource.Resources.MobilePhone</span>
                                                    &nbsp;<span style="color:rgb(121, 85, 72);">W - @Fingerprints.LocalResource.Resources.WorkPhone</span>
                                                </label>
                                            </div>

                                            <table class="col-md-12 glossy-table table-striped table-condensed cf table-change ersea-table">
                                                <thead class="table-hd scroll-thead">
                                                    <tr>
                                                        <th>@Fingerprints.LocalResource.Resources.Client</th>
                                                        <th>@Fingerprints.LocalResource.Resources.ParentName</th>
                                                        <th>@Fingerprints.LocalResource.Resources.PhoneNumber</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="ersea-table-head glossy-table-body bind_body scroll-tbody" id="tbody-bounce-email"></tbody>
                                            </table>
                                        </div>
                                    </div>


                                    <div class="tab-pane fade" id="Noemails">

                                        <div class="col-xs-12" style="padding:15px;">
                                            <div class="col-xs-12 text-left" style="margin-bottom:10px;font-size:12px;padding:0;">
                                                <label>
                                                    <span style="color:rgb(63, 81, 181);margin-right: 10px;">H - @Fingerprints.LocalResource.Resources.HomePhone</span>
                                                    &nbsp;<span style="color:rgb(0, 150, 136);margin-right: 10px;">M - @Fingerprints.LocalResource.Resources.MobilePhone</span>
                                                    &nbsp;<span style="color:rgb(121, 85, 72);">W -@Fingerprints.LocalResource.Resources.WorkPhone</span>
                                                </label>
                                            </div>

                                            <table class="col-md-12 glossy-table table-striped table-condensed cf table-change ersea-table">
                                                <thead class="table-hd scroll-thead">
                                                    <tr>
                                                        <th>@Fingerprints.LocalResource.Resources.Client</th>
                                                        <th>@Fingerprints.LocalResource.Resources.ParentName</th>
                                                        <th>@Fingerprints.LocalResource.Resources.PhoneNumber</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="ersea-table-head glossy-table-body bind_body scroll-tbody" id="tbody-noemails"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>



                    </div>

                </form>


            </div>
        </div>
    </div>

</div>