        function bindGridQues(dataQuesDetail, num_cols) {
          
            rounds = 0;
            $('#gridquestion > tbody > tr').remove();
            if (dataQuesDetail.listQues.length == 0) {
                $('#div1').show();
            }
            else {
                $('#div1').hide();
                var tbody = $('#gridquestion > tbody');
                for (var i = 0; i < dataQuesDetail.listQues.length; i++) {
                    var Question = "CustomQues[" + rounds + "].Question";
                    var QuesYes = "CustomQues[" + rounds + "].QuesYes";
                    var QuesNo = "CustomQues[" + rounds + "].QuesNo";
                    var CQID = "CustomQues[" + rounds + "].CQID";
                    $('#gridquestion').show();
                    var tr = "";
                    tr += "<tr><style='text-align:center;'>";
                   
                    tr = tr + '<td><input  name=' + Question + ' class = "form-control" type="text" value="' + dataQuesDetail.listQues[i].Question + '" ></td>';
                    tr = tr + '<td><input  name=' + QuesYes + ' class = "form-control" type="text" value="' + dataQuesDetail.listQues[i].QuesYes + '" ></td>';
                    tr = tr + '<td><input  name=' + QuesNo + ' class = "form-control" type="text" value="' + dataQuesDetail.listQues[i].QuesNo + '" ></td>';
                       
                    tr = tr + '<td><input type="hidden"  name="' + CQID + '" value="' + dataQuesDetail.listQues[i].CQID + '" /><a href="javascript:void(0)" onclick="RemoveQues(this);" accesskey="' + dataQuesDetail.listQues[i].CQID + '" style=" text-decoration:underline;">Delete</a></td></tr>';
                    $('#gridquestion > tbody').append(tr);
                    //$('#gridquestion > tbody tr').each(function () {
                    //    var y = $(this).find('td:eq(1) :input[type="text"]').mask("(999) 999-9999");
                    //});
                    rounds = rounds + 1;
                }
            }
        }




