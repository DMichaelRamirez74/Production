        $('#btnSubmitPIRStaff').on('click', function () {

            if ($('#filltable').find('.scroll-tbody').find('input[class=pirCheck]:checked').length == 0) {
                customAlert('Please select the checkbox to display Section B in PIR');
                return false;
            }

            var model = {};

            var arr = [];

            $('#filltable').find('.scroll-tbody').find('input[class=pirCheck]').each(function (i, checkbox) {

                arr.push({

                    'UserId': $(checkbox).closest('tr').find('td').eq(0).children('.pirUserId').val(),
                    'RoleId': $(checkbox).closest('tr').find('td').eq(1).children('.pirRoleId').val(),
                    'IsShowSectionB': $(checkbox).is(':checked') ? true : false,
                    'IsShowPIR': true
                });

            });

            model = {
                'PIRStaffsList': arr
            };

            $('#spinner').show();

            $.ajax({
                url:  HostedDir + "/PIR/InsertPIRSectionBAccessStaffs",
                datatype: 'json',
                type: 'post',
                data: { pirAccessStaffs: JSON.stringify(model) },
                success: function (data) {
                    $('#spinner').hide();
                    if (data) {

                        customAlert('Recore saved successfully');
                    }
                },
                error: function (data) {
                    $('#spinner').hide();
                    customAlert("Session Ended Log Onto The System Again."); setTimeout(function () { window.location.href = HostedDir + '/login/Loginagency'; }, 2000);

                }
            });

        });

    });

