            }
        }




        if ($('#ChangeclientStatus').find('#WithdrawnTypeDiv').is(':visible') && $('#withdrawalTypeSelect').val() != '0') {
            var withdraVal = $('#withdrawalTypeSelect').val();

            switch (withdraVal) {
                case '2':
